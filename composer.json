{
    "name": "tightenco/mise",
    "description": "Mise: A tool for installing Laravel recipes.",
    "keywords": [
        "cli"
    ],
    "homepage": "https://github.com/tighten/mise",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/tighten/mise/issues",
        "source": "https://github.com/tighten/mise"
    },
    "authors": [
        {
            "name": "Matt Stauffer",
            "email": "matt@tighten.co"
        },
        {
            "name": "Jon Sugar"
        }
    ],
    "require": {
        "php": "^8.2.0"
    },
    "require-dev": {
        "laravel-zero/framework": "^11.36.1",
        "illuminate/log": "^11.5",
        "illuminate/queue": "^v11.44.2",
        "laravel/pint": "^1.18.3",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.7.1",
        "spatie/ray": "^1.41.5",
        "tightenco/duster": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "builds/mise"
    ]
}
